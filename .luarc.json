{
  "$schema": "https://raw.githubusercontent.com/sumneko/vscode-lua/master/setting/schema.json",
  "runtime": {
    "version": "LuaJIT"
  },
  "diagnostics": {
    "globals": ["vim"]
  },
  "workspace": {
    "library": ["/usr/share/nvim/runtime", "~/.local/share/nvim/lazy/lazy.nvim/lua", "~/.config/nvim/lua", "${3rd}/luassert/library" ],
    "checkThirdParty": "Disable"
  },
  "telemetry": {
    "enable": false
  }
}
